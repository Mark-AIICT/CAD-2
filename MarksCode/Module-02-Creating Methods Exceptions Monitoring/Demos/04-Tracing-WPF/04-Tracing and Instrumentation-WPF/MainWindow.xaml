        <Button Content="Send to Trace" HorizontalAlignment="Left" VerticalAlignment="Top" Width="198" Margin="32,130,0,0" RenderTransformOrigin="1.217,0.7" Click="Button_Click"/>
        <TextBox x:Name="TextBox1" HorizontalAlignment="Left" Height="24" Margin="130,72,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="362" RenderTransformOrigin="0.088,-1.542"/>
        <Label Content="Text to Trace:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="32,68,0,0"/>

    </Grid>
</Window>
